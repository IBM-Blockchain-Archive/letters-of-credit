			"title": {
				"$ref": "#/messages/customtool.documentationUrl"
			},
			"key": "documentationUrl"
		},
		{
			"type": "text",
			"readonly": true,
			"title": {
				"$ref": "#/messages/customtool.name"
			},
			"key": "name"
		},
		{
			"type": "text",
			"readonly": true,
			"title": {
				"$ref": "#/messages/customtool.dashboard_url"
			},
			"key": "dashboard_url"
		},
		{
			"type": "text",
			"readonly": true,
			"title": {
				"$ref": "#/messages/customtool.description"
			},
			"key": "description"
		},
		{
			"type": "text",
			"readonly": true,
			"title": {
				"$ref": "#/messages/customtool.additional-properties"
			},
			"key": "additional-properties"
		}
